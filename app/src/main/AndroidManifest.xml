<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.untitleddevelopments.wintecdegreeplanner">

    <application
        android:name=".DB.ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".global.PrefsTestActivity"></activity>
        <activity android:name=".tests.DBTestActivity" />
        <activity
            android:name=".GeoffsSandpit.GeoffTest"
            android:label="@string/title_activity_geoff_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- cw apps -->
        <activity
            android:name=".otsStudentSignup"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".otsDisclaimer"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".otsStudentAdmin"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".otsStudentDone"
            android:theme="@style/AppTheme.NoActionBar" />


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>